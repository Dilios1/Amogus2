void question()
{
    int ready;
    printf("ARE YOU READY TO GO BACK?\n");
    printf("1.YES\n"
           "2.NO\n");
    scanf("\n%d",&ready);

    if(ready==1)
    {
        ++boss1;
        start_zone();

    }
    else if(ready==2)
        question();



}

void alegere2()
{
    int questsat;
    printf("Loading....");
    system("cls||clear");
    Sleep(1);
    printf("....."); //POVESTE LA CUM A AJUNS CARACTERUL AICI
    system("cls||clear");
    Sleep(1);
    printf("IMAGINE SAT");
    system("cls||clear");
    Sleep(1);
    printf("Descriere sat\n"
           "Vrei sa ii ajuti?\n"
           "1.Da\n"
           "2.Nu\n");
    scanf("%d",&questsat);
    if(questsat==1)
        helpsat();
    else if(questsat==2)
        nothelp();
}
void helpsat()
{
    int alegeresat;
    system("cls||clear");
    Sleep(1);
    printf("Du-te omoara niste mobi pentru sateni\n"
           "In padure ai omorat 5 lupi\n"
           "Ai luat de pe jos 5 bucati de carne\n"
           "Cand te ai dus inapoi in sat, ai realizat ca sunt atacati");
    printf("Vrei sa ii ajuti sa lupte contra hoti?\n");
    printf("1.Da\n"
           "2.Fugi\n");
    scanf("%d",&alegeresat);
    if(alegeresat==1)
        fightback();
    else if(alegeresat==2)
    {
        printf("lol pussy you ran");
    }
}
void fightback() //LUPTA DIN SAT
{
    int bataie;
    system("cls||clear");
    Sleep(1);
    printf("Ai 3 inamici in fata\n"
           "Unul dintre ei da charge spre tine\n"
           "Ce faci?\n"
           "1.Dai dodge la stanga\n"
           "2.Dai dodge la dreapta\n");
    scanf("%d",&bataie);
    if(bataie==1)
    {
        int choicebataie;
        printf("You didn't dodge the hit\n"
               "Now your head is at your feet\n"
               "Do you wanna try again?\n"
               "1.Yes\n"
               "2.Not yet\n");
        scanf("%d",&choicebataie);
        if(choicebataie==1)
        {
            system("cls||clear");
            Sleep(1);
            fightback();
        }
        else if(choicebataie==2)
        {
            loop_fight();
        }
    }
    else if(bataie==2)
        dodge_hit();
}
void loop_fight()
{
    system("cls||clear");
    Sleep(1);
    int choicebataie;
    printf("You didn't dodge the hit\n"
           "Now your head is at your feet\n"
           "Do you wanna try again?\n"
           "1.Yes\n"
           "2.Not yet\n");
    scanf("%d",&choicebataie);
    if(choicebataie==1)
    {
        system("cls||clear");
        Sleep(1);
        fightback();
    }
    else if(choicebataie==2)
    {
        loop_fight();
    }
}
void nothelp()
{
    system("cls||clear");
    Sleep(1);
    printf("LOL GET FUCKED VILLIAGERS");
}
void dodge_hit()
{
    Sleep(3000);
    system("cls||clear");
    printf("You succesfully dodge the hit and then followed it up with a clear hit on two of the orcs and afterwards you quickly charged the 3rd one as well and killed it."
           "\n\nThe villagers were happy that you helped them and they thanked you by telling you were to go forward to the right hand of the Warchief.\n They even had a secret path that wasnt guarded"
    "\n The way there was very easy to reach. You were very happy that you managed to save the troll village and that they helped you reach the Warchief's right hand man" );

     boss_fight2();


}


void boss_fight2()
{
  system("cls||clear");
    Sleep(1);
    int choiceboss2;
    printf("Karguk the Destroyer jumps in the air lunging his weapon forward\n"
           "Where do you wanna dodge?\n"
           "1.Backwards\n"
           "2.Forwards\n");

     scanf("%d",&choiceboss2);

     if(choiceboss2==1)
     {
         loop_bossfight2();
     }
     else if(choiceboss2==2)
     {
        boss_fight22();
     }


}


void loop_bossfight2()
{
  system("cls||clear");
    Sleep(1);
    int loopboss2;
  printf("You died,go agian?\n"
          "1.Yes\n"
           "2.Not yet\n");
    scanf("%d",&loopboss2);
    if(loopboss2==1)
        boss_fight2();
    else if(loopboss2=2)
     loop_bossfight2();


}


void boss_fight22()
{
    int part2boss;
    printf("After missing his shot and getting his sword stuck in the mud,Karguk is standing with his back towards you\n"
           "Where you wanna strike him?\n"
           "1.Leg"
           "2.Hand");
    scanf("%d",&part2boss);
    if(part2boss==1)
    {
        printf("You missed his leg, you know what that means...");
        loop_bossfight2();
    }
    else if(part2boss==2)
    {

        printf("You quickly slashed his hands and then his head. You were victorious\n"
               "This was clearly a hard fight, you almost lost twice and were exhausted\n"
               "You picked up his armor and while you were searching around you found a key, might be useful later\n");


    Sleep(3000);
   system("cls||clear");


      int ready2;
    printf("ARE YOU READY TO GO BACK?\n");
    printf("1.YES\n"
           "2.NO\n");
    scanf("\n%d",&ready2);

    if(ready2==1)
    {
        ++boss2;
        start_zone();


    }
    else if(ready2==2)
        question2();


    }

    }
